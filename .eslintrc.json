{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "es6": true,
    "node": true,
    "webextensions": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "linebreak-style": 0,
    "import/no-named-as-default": 0,
    "react/destructuring-assignment": 0,
    "react/no-access-state-in-setstate": 0,
    "no-param-reassign": 0
  },
  "parser": "babel-eslint"
}
