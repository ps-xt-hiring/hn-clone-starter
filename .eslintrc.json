{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "linebreak-style": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "enforceDynamicLinks": 0
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
